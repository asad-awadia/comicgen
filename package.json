{
  "name": "comicgen",
  "version": "1.9.4",
  "description": "Create comics for your website or app",
  "main": "src/comicgen.js",
  "jsdelivr": "src/comicgen.js",
  "unpkg": "src/comicgen.js",
  "repository": "https://github.com/gramener/comicgen",
  "author": "S Anand <s.anand@gramener.com>",
  "bin": {
    "comicserver": "./src/comicserver.js"
  },
  "scripts": {
    "build": "rimraf dist && node src/buildchars.js",
    "lint": "eclint . && eslint --ext js,html . && npm audit --omit=dev",
    "prepublishOnly": "npm run lint",
    "postinstall": "npm run build && node src/speechbubble.js",
    "start": "node src/comicserver.js"
  },
  "license": "MIT",
  "private": false,
  "dependencies": {
    "body-parser": "1",
    "cheerio": "1.0.0-rc.10",
    "color-interpolate": "1",
    "d3-interpolate-path": "2",
    "express": "4",
    "fontkit": "2",
    "get-google-fonts": "1",
    "glob": "8",
    "lodash": "4",
    "mustache": "4",
    "rimraf": "3",
    "roughjs": "^4.5.2",
    "sharp": "^0.30.7",
    "winston": "^3.8.1",
    "winston-daily-rotate-file": "^4.7.1"
  },
  "devDependencies": {
    "eclint": "^2.8.1",
    "eslint": "^7.32.0",
    "eslint-plugin-html": "^6.2.0"
  }
}
